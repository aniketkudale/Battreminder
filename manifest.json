{
  "update_url": "https://clients2.google.com/service/update2/crx",
  "name": "Battreminder",
  "version": "1.0",
  "description": "Reminds you when your laptop battery is fully charged!",
  "icons": {
    "16": "icons/icon16.png",
    "32": "icons/icon32.png",
    "48": "icons/icon48.png",
    "96": "icons/icon96.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "notifications"
  ],
  "browser_action": {
    "default_icon": "icons/icon128.png"
  },
  "manifest_version": 2,
  "background": {
    "scripts": [
      "js/background.js"
    ]
  }
}